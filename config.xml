<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.carimobil" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CariMobil</name>
    <description>
        MVP dari aplikasi untuk memesan mobil operasional/pool
    </description>
    <author email="trirawibowo@gmail.com" href="">
        Tri R.A. Wibowo
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/icon/carimobil32x32.png" density="ldpi" />
        <icon src="www/icon/carimobil72x72.png" density="mdpi" />
        <icon src="www/icon/carimobil128x128.png" density="hdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="LoadingDialog" value="CariMobil,Loading..."/>
    <preference name="xwalkVersion" value="15+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
